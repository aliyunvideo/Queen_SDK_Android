<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#88000000"
    tools:context=".examples.advanced.LiveStreaming">

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:padding="6dp"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="uid"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_uid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="event"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_event"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="elapsedTime"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_elapsedTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="start2JoinChannel"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_start2JoinChannel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="join2JoinSuccess"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_join2JoinSuccess"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="joinSuccess2RemoteJoined"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_joinSuccess2RemoteJoined"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="remoteJoined2SetView"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_remoteJoined2SetView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="remoteJoined2UnmuteVideo"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_remoteJoined2UnmuteVideo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

        <TableRow>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="remoteJoined2PacketReceived"
                android:textColor="@android:color/white" />

            <TextView
                android:id="@+id/tv_remoteJoined2PacketReceived"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:minWidth="80dp"
                android:textColor="@android:color/white"
                tools:text="20ms" />

        </TableRow>

    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>